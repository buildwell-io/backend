{
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@core/config": [ "src/app/core/config/index" ],
      "@core/database": [ "src/app/core/database/index" ],
      "@core/decorators": [ "src/app/core/decorators/index" ],
      "@core/filters": [ "src/app/core/filters/index" ],
      "@core/global": [ "src/app/core/global/index" ],
      "@core/interfaces": [ "src/app/core/interfaces/index" ],
      "@core/mail": [ "src/app/core/mail/index" ],
      "@core/swagger": [ "src/app/core/swagger/index" ],
      "@core/throttle": [ "src/app/core/throttle/index" ],
      "@core/utils": [ "src/app/core/utils/index" ],
      "@modules/account": [ "src/app/modules/account/index" ],
      "@modules/authentication": [ "src/app/modules/authentication/index" ],
      "@modules/authorization": [ "src/app/modules/authorization/index" ],
      "@modules/widget": [ "src/app/modules/widget/index" ]
    },
    "esModuleInterop": true
  },
  "files": [ ],
  "include": [ ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
