{
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@app/config": [ "src/app/config/index" ],
      "@app/constants": [ "src/app/constants/index" ],
      "@app/controllers": [ "src/app/controllers/index" ],
      "@app/database": [ "src/app/database/index" ],
      "@app/decorators": [ "src/app/decorators/index" ],
      "@app/enums": [ "src/app/enums/index" ],
      "@app/filters": [ "src/app/filters/index" ],
      "@app/guards": [ "src/app/guards/index" ],
      "@app/interfaces": [ "src/app/interfaces/index" ],
      "@app/providers": [ "src/app/providers/index" ],
      "@app/services": [ "src/app/services/index" ],
      "@app/strategies": [ "src/app/strategies/index" ],
      "@app/swagger": [ "src/app/swagger/index" ],
      "@app/utilities": [ "src/app/utilities/index" ]
    },
    "esModuleInterop": true
  },
  "files": [ ],
  "include": [ ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
